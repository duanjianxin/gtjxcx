.img_box {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.img_box image {
  width: 100%;
  height: 100vh;
}
.tableWarp {
  z-index: 5;
  position: absolute;
  top: 24rpx;
  left: 0;
  right: 0;
  margin: auto;
  width: 384rpx;
  background: #888899;
  border: 1rpx solid rgba(136, 136, 153, 0.5);
  border-radius: 8rpx;
}
.tableWarp button {
  font-size: 32rpx;
  float: left;
  color: #ffffff;
  width: 50%;
  height: 76rpx;
}
.tableWarp button.select {
  background: #1177e7;
}
.tableWarp button[plain] {
  border: none;
}
.mapWarp .locationBtn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 76rpx;
  height: 76rpx;
  position: absolute;
  top: 528rpx;
  right: 24rpx;
  margin: auto;
}
.mapWarp .scaleBox {
  width: 76rpx;
  position: absolute;
  top: 630rpx;
  right: 24rpx;
  margin: auto;
}
.mapWarp .scaleBox button {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 76rpx;
}
.mapWarp .shapeBox {
  width: 76rpx;
  position: absolute;
  top: 424rpx;
  right: 24rpx;
  margin: auto;
}
.mapWarp .shapeBox button {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 76rpx;
}
.mapWarp .delBox {
  width: 638rpx;
  background: #ffffff;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 22rpx;
  margin: auto;
  padding: 32rpx;
}
.mapWarp .delBox .satelliteInformation {
  height: 896rpx;
  overflow-y: scroll;
}
.mapWarp .delBox .satelliteInformation .title {
  text-align: left;
  color: rgba(0, 0, 0, 0.87);
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #aaaabb;
}
.mapWarp .delBox .satelliteInformation .Close {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.mapWarp .delBox .satelliteInformation .centorBox {
  font-size: 24rpx;
  line-height: 32rpx;
  color: #5f6164;
}
.mapWarp .delBox .satelliteInformation .centorBox .centorBoxRow {
  margin: 16rpx auto;
}
.mapWarp .delBox .satelliteInformation .centorBox .centorBoxRow .name {
  width: 288rpx;
  float: left;
  white-space: normal;
  text-align: left;
}
.mapWarp .delBox .satelliteInformation .centorBox .centorBoxRow .value {
  width: 306rpx;
  float: right;
  white-space: normal;
  text-align: right;
}
.mapWarp .personnelDelWarp {
  width: 638rpx;
  background: #ffffff;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 22rpx;
  margin: auto;
  padding: 32rpx;
  z-index: 3;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel {
  position: relative;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel .left {
  float: left;
  display: flex;
  align-items: center;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel .left .headImg {
  width: 100rpx;
  height: 100rpx;
  margin-right: 32rpx;
  float: left;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel .left .del .name {
  font-size: 28rpx;
  color: #37373d;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel .left .del .distance {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #5f6164;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnel .more {
  float: right;
  position: absolute;
  top: 0;
  bottom: 0;
  right: 32rpx;
  margin: auto;
  display: flex;
  align-items: center;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnelDel {
  margin-top: 24rpx;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnelDel .del {
  border-top: 1rpx solid #aaaabb30;
  border-bottom: 1rpx solid #aaaabb30;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnelDel .del .lineDelBox {
  margin: 12rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  color: #5f6164;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnelDel .btnList {
  float: right;
  margin-top: 22rpx;
}
.mapWarp .personnelDelWarp .personnelDelBox .personnelDel .btnList .iconfont {
  width: 76rpx;
  height: 76rpx;
  margin-left: 24rpx;
  float: left;
  border-radius: 38rpx;
  border: none;
  color: #3178ea;
  line-height: 76rpx;
  font-size: 76rpx;
}
